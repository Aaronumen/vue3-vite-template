    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.vue,.ts,.tsx --fix"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "dayjs": "^1.11.6",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.2",
    "pinia": "^2.0.25",
    "vue": "^3.2.41",
    "vue-router": "4"
  },
  "lint-staged": {
    "src/**/*.{js,json,vue,ts,tsx}": [
